.text-view, .panel
	.text
		position relative
		display inline-block
		width 205px
		margin-right 15px

		font-family textFont

		line-height 22px
		.line-numbers
			position absolute
			top 0
			left -1.5em
			line-height 22px
			font-size 10px
			color medium-grey

	.annotations
		display inline-block
		width 100px
		font-size 12px
		font-family sans-serif
		color medium-grey
		ol
			list-style none
			padding-left 0
			li
				font-size 12px
				margin-top 10px