@import './import/*'

@media screen
	html, body
		font-size 12px
		font-family sans-serif
		margin 0
		padding 0
		overflow hidden
		height 100%
		
	#main
		height 100%

	a
		color skyBlue
		text-decoration none
		font-family sans-serif

	button
		background none
		box-shadow none
		// outline none
		border 1px solid #CCC
		border-radius 3px
		color #B325A0
		cursor pointer
		line-height 24px
		width 100%

	.wrapper
		margin 0 auto
		
	@import '../coffee/views/entry/*'

div.facets div.facet.range div.placeholder div.body div.slider button
	top -14px
	right -35px

.info-icon
	display inline-block
	width 14px
	height 14px
	margin-left 4px
	position relative
	top 1px
	background-size 100%
	background-image: url('data:image/png;base64,')