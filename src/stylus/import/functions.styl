easeOutQuint()
	-webkit-transition: all 600ms cubic-bezier(0.23, 1, 0.32, 1);
	transition:         all 600ms cubic-bezier(0.23, 1, 0.32, 1);

vendor(prop, args)
	-webkit-{prop} args
	-moz-{prop} args
	-ms-{prop} args
	-o-{prop} args
	{prop} args

border-radius()
	vendor('border-radius', arguments)

background-image()
	vendor('background-image', arguments)

clear-fix()
	&:before, &:after
		clear both
		content ""
		display block
		visibility hidden
		line-height 0
		height 0

no-padding-margin()
	padding 0
	margin 0

.not-visible
	visibility hidden
	opacity 0

.hidden
	display none